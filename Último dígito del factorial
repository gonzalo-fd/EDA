#include <iostream>
using namespace std;

int factorial(int n){

if(n == 0 || n == 1)
	return 1;
else if (n ==2 || n == 4)
	return n;
else if(n==3)
	return 6;
else if(n > 4 )
	return 0;

}


void resuelve(){

int casos, n;

cin >> casos;

for (int i = 0; i < casos; ++i)
{
	cin>> n;
	cout << factorial(n) << endl;
}

}

int main(){

resuelve();
	return 0;
}
